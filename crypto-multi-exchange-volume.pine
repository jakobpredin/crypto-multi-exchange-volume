bitstampVolume = bitstampTicker != "GOLD" ? nz(security(bitstampTicker, timeframe.period, volume)) : 0
bittrexVolume = bittrexTicker != "GOLD" ? nz(security(bittrexTicker, timeframe.period, volume)) : 0
coinbaseVolume = coinbaseTicker != "GOLD" ? nz(security(coinbaseTicker, timeframe.period, volume)) : 0
geminiVolume = geminiTicker != "GOLD" ? nz(security(geminiTicker, timeframe.period, volume)) : 0
krakenVolume = krakenTicker != "GOLD" ? nz(security(krakenTicker, timeframe.period, volume)) : 0
poloniexVolume = poloniexTicker != "GOLD" ? nz(security(poloniexTicker, timeframe.period, volume)) : 0

baseVolume = binanceVolume + bitfinexVolume + bitstampVolume + bittrexVolume + coinbaseVolume + geminiVolume + krakenVolume + poloniexVolume
baseMA = ema(baseVolume, emaLength)
quoteVolume = baseVolume * ((high+low+close)/3) // Uses HLC average price approximation
quoteMA = ema(quoteVolume, emaLength)

totalVolume = useQuoteVolume ? quoteVolume : baseVolume
MA = useQuoteVolume ? quoteMA : baseMA

// PLOT CHARTS
plot(series=totalVolume, title="Volume", style=plot.style_histogram, linewidth=1, transp=0, color=color.white)
plot(series=MA, title="Moving Average", style=plot.style_linebr, linewidth=1, transp=20, color=color.white)
